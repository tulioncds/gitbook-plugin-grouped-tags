{
    "name": "gitbook-plugin-grouped-tags",
    "description": "Add grouped tags to GitBook",
    "version": "1.0.3",
    "engines": {
        "gitbook": "*"
    },
    "homepage": "https://github.com/tulioncds/gitbook-plugin-grouped-tags",
    "dependencies": {
      "github-slugid": "^1.0.1"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tulioncds/gitbook-plugin-grouped-tags.git"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/tulioncds/gitbook-plugin-grouped-tags/issues"
    },
    "gitbook": {
        "properties": {
            "placement": {
                "type": "string",
                "description": "placement to display tags on the page",
                "default": "top"
            },
            "groups": {
                "type": "object",
                "description": "Groups of tags",
                "default": [
                    {"placeholder": "tags", "legend": ""}
                ]
            }
        }
    },
    "keywords": [
      "gitbook",
      "plugin",
      "tag"
    ],
    "author": "Bill Ryan <yuanbin2014@gmail.com>"
}
